		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@versatiles/svelte": "^0.1.2",
		"globals": "^15.11.0",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"svelte-highlight": "^7.7.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.6.3",
		"vite": "^5.4.10"
	}
